{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This template creates a 5G Infra test environment with four separate VPCs: eNB, MEC, CP, and GiLAN.",
    "Metadata": {
        "Version" : "2.0",
        "Comment" : "This template configures VPC network with the following capabilities:",
        "Comment" : "  - Create four VPCs with each containing public and private subnets.",
        "Comment" : "  - Enable testing of inter-VPC ping between the VPC subnets",
        "Comment" : "  - Enable computers to ssh to VMs in the public subnets and private subnets (via ssh agent forwarding)",
        "Comment" : "  - Enable VMs in the private subnets to access Internet",
        "Comment" : "This template is to be enhanced based on the 5G traffic requirements:",
        "Comment" : "  - ACL protection for the subnets",
        "Comment" : "  - Inter-VPC subnet routing",

        "AWS::CloudFormation::Interface": {
            "ParameterGroups": [
                {
                    "Label": {
                        "default": "VPC Configuration"
                    },
                    "Parameters": [
                        "AvailabilityZones"
                    ]
                },
                {
                    "Label": {
                        "default": "EC2 Key Pair Selection"
                    },
                    "Parameters": [
                        "KeyPairName"
                    ]
                },
                {
                    "Label": {
                        "default": "VPC Network Configuration"
                    },
                    "Parameters": [
                        "VPCeNBCIDR",
                        "VPCeNBPubSubnet1CIDR",
                        "VPCeNBPrvSubnetMgmtCIDR",
                        "VPCeNBPrvSubnetS11MMECIDR",
                        "VPCeNBPrvSubnetS1UCIDR",
                        "VPCeNBTenancy",
                        "VPCMECCIDR",
                        "VPCMECPubSubnet1CIDR",
                        "VPCMECPrvSubnetMgmtCIDR",
                        "VPCMECPrvSubnetZMQCIDR",
                        "VPCMECPrvSubnetS1UCIDR",
                        "VPCMECPrvSubnetSGICIDR",
                        "VPCMECTenancy",
                        "VPCCPCIDR",
                        "VPCCPPubSubnet1CIDR",
                        "VPCCPPrvSubnetMgmtCIDR",
                        "VPCCPPrvSubnetS11MMECIDR",
                        "VPCCPPrvSubnetZMQCIDR",
                        "VPCCPTenancy",
                        "VPCGiLANCIDR",
                        "VPCGiLANPubSubnet1CIDR",
                        "VPCGiLANPrvSubnetMgmtCIDR",
                        "VPCGiLANPrvSubnetSGICIDR",
                        "VPCGiLANTenancy"
                    ]
                }
            ],
            "ParameterLabels": {
                "AvailabilityZones": {
                    "default": "Availability Zones"
                },
                "KeyPairName": {
                    "default": "Key pair name"
                },
                "VPCeNBCIDR": {
                    "default": "VPC-eNB CIDR"
                },
                "VPCeNBPubSubnet1CIDR": {
                    "default": "VPC-eNB Public subnet 1 CIDR"
                },
                "VPCeNBPrvSubnetMgmtCIDR": {
                    "default": "VPC-eNB Private subnet Mgmt CIDR"
                },
                "VPCeNBPrvSubnetS11MMECIDR": {
                    "default": "VPC-eNB Private subnet S11-MME CIDR"
                },
                "VPCeNBPrvSubnetS1UCIDR": {
                    "default": "VPC-eNB Private subnet S1U CIDR"
                },
                "VPCeNBTenancy": {
                    "default": "VPC-eNB Tenancy"
                },
                "VPCMECCIDR": {
                    "default": "VPC-MEC CIDR"
                },
                "VPCMECPubSubnet1CIDR": {
                    "default": "VPC-MEC Public subnet 1 CIDR"
                },
                "VPCMECPrvSubnetMgmtCIDR": {
                    "default": "VPC-MEC Private subnet Mgmt CIDR"
                },
                "VPCMECPrvSubnetZMQCIDR": {
                    "default": "VPC-MEC Private subnet ZMQ CIDR"
                },
                "VPCMECPrvSubnetS1UCIDR": {
                    "default": "VPC-MEC Private subnet S1U CIDR"
                },
                "VPCMECPrvSubnetSGICIDR": {
                    "default": "VPC-MEC Private subnet SGI CIDR"
                },
                "VPCMECTenancy": {
                    "default": "VPC-MEC Tenancy"
                },
                "VPCCPCIDR": {
                    "default": "VPC-CP CIDR"
                },
                "VPCCPPubSubnet1CIDR": {
                    "default": "VPC-CP Public subnet 1 CIDR"
                },
                "VPCCPPrvSubnetMgmtCIDR": {
                    "default": "VPC-CP Private subnet Mgmt CIDR"
                },
                "VPCCPPrvSubnetS11MMECIDR": {
                    "default": "VPC-CP Private subnet S11-MME CIDR"
                },
                "VPCCPPrvSubnetZMQCIDR": {
                    "default": "VPC-CP Private subnet ZMQ CIDR"
                },
                "VPCCPTenancy": {
                    "default": "VPC-CP Tenancy"
                },
                "VPCGiLANCIDR": {
